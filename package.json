{
  "name": "t3chat-clone",
  "version": "1.0.0",
  "description": "A modern, feature-rich AI chat application built with Vue.js, featuring multiple LLM providers, real-time streaming, and Google authentication.",
  "author": "T3 Chat Clone Contributors",
  "license": "MIT",
  "homepage": "https://github.com/your-username/t3chat-clone#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/t3chat-clone.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/t3chat-clone/issues"
  },
  "keywords": [
    "vue",
    "typescript",
    "ai",
    "chat",
    "openai",
    "gemini",
    "supabase",
    "tailwindcss",
    "pinia"
  ],
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "clean": "rm -rf dist node_modules/.vite",
    "prepare": "npm run lint && npm run type-check",
    "ci": "npm ci && npm run lint:check && npm run type-check && npm run build"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@supabase/supabase-js": "^2.50.0",
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "google-auth-library": "^10.0.0",
    "lucide-vue-next": "^0.514.0",
    "pinia": "^3.0.1",
    "postcss": "^8.5.4",
    "prismjs": "^1.30.0",
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.14.0",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.22.0",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^7.0.2",
    "prettier": "3.5.3",
    "typescript": "~5.8.0",
    "vite": "^6.2.4",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
